@startuml class

object Marker {
  label: varchar
  lat: double
  lng: double
}

object MarkerType {
  code: varchar
  label: varchar
}

object MarkerOption {
  code: varchar
  label: varchar
}

object User {
  ' id: integer PK
  name: varchar(50)
  email: varchar
  password: varchar
}

object Role {
  ' id: integer PK
  code: varchar
  label: varchar
}

User "0..*" -- "1" Role
Marker "0..*" -- "1" MarkerType
Marker "0..*" -- "0..*" MarkerOption

' Relationship "0..*" -- "1" User_RelationshipType
' RelationshipType "1" -- "0..*" User_RelationshipType
' User_RelationshipType "0..*" -- "1" User
' Payment "0..*" -- "1" Relationship
' Payment "0..*" -- "1" User
' Payment "1" -- "0..*" ReminderDate
@enduml